import("//fusl/fusl.gni")

fusl_source_set("internal") {
  sources = [
    "floatscan.c",
    "intscan.c",
    "libc.c",
    "procfdname.c",
    "shgetc.c",
    "syscall_ret.c",
    "vdso.c",
    "version.c",
  ]

  if (current_cpu == "x64") {
    sources += [ "x86_64/syscall.s" ]
  } else {
    sources += [ "syscall.c" ]
  }
}
