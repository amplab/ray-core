[DartPackage="mojo_services"]
module mojo.media;

// Describes media.
struct MediaMetadata {
  // Duration in nanoseconds.
  uint64 duration;

  string? title;

  string? artist;

  string? album;

  string? publisher;

  string? genre;

  string? composer;
};
